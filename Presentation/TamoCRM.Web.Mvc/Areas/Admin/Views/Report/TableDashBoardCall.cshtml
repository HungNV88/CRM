                                <td style="width:62px">@j._khung16_tyle4</td>
                                <td style="width:62px">@j._khung17_tyle4</td>
                                <td style="width:62px">@j._khung18_tyle4</td>
                                <td style="width:62px">@j._khung19_tyle4</td>
                                <td style="width:62px">@j._khung20_tyle4</td>
                            </tr>
                            <tr>
                                <td style="width:199px">Cuộc gọi đầu tiên (lúc)</td>
                                <td style="width:40px"></td>
                                <td style="width:62px">@j._khung8_tyle5</td>
                                <td style="width:62px">@j._khung9_tyle5</td>
                                <td style="width:62px">@j._khung10_tyle5</td>
                                <td style="width:62px">@j._khung11_tyle5</td>
                                <td style="width:62px">@j._khung12_tyle5</td>
