                                <td style="width:40px">@j.Sum_tyle2</td>
                                <td style="width:62px">@j._khung8_tyle2</td>
                                <td style="width:62px">@j._khung9_tyle2</td>
                                <td style="width:62px">@j._khung10_tyle2</td>
                                <td style="width:62px">@j._khung11_tyle2</td>
                                <td style="width:62px">@j._khung12_tyle2</td>
                                <td style="width:62px">@j._khung13_tyle2</td>
                                <td style="width:62px">@j._khung14_tyle2</td>
                                <td style="width:62px">@j._khung15_tyle2</td>
                                <td style="width:62px">@j._khung16_tyle2</td>
